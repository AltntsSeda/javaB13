package day3variable;

public class Variabledecleration {

	public static void main(String[] args) {
		
      //declaration
	  byte inches;
      int speed;
	  short month;
	  float salesCommision;
	  double distance;
		
	  //assigment
	  inches=5;
	  speed=200;
	  month=2;
	  distance=40.2;
	  salesCommision=5;
	  //declared+assigned
	  byte inches2=5; 
	   
	  System.out.println(inches);
	}

}
